{
  "extends": "stylelint-config-standard",
  "rules": {
    "selector-type-no-unknown": [
      true, {
        "ignoreTypes": [
          "ion-badge", "ion-card-header", "ion-card-title", "ion-content",
          "ion-icon", "ion-item", "ion-list", "ion-list-header", "ion-navbar",
          "ion-slide", "ion-slides", "ion-thumbnail", "ion-title",
          "ion-toolbar",
          "jc-member", "jc-mission", "jc-skill"
        ]
      }
    ]
  }
}
