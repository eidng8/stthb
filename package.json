{
  "name": "startrek-timelines-handbook",
  "author": "eidng8",
  "description": "Star Trek Timelines Handbook",
  "license": "CC-BY-SA-4.0",
  "keywords": [
    "ionic2",
    "angular2",
    "roolup",
    "webpack",
    "yarn",
    "typescript",
    "karma",
    "protractor",
    "istanbul",
    "gitlab CI"
  ],
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "node sh/e2e.js",
    "e2e:interactive": "node sh/e2e.js --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic build ios --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic build android --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@ionic/storage": "1.1.7",
    "ionic-angular": "2.0.0-rc.6",
    "ionic-native": "2.4.1",
    "ionicons": "3.0.0",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26",
    "sw-toolbox": "3.4.0",
    "lodash-es": "^4.17.4",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.0.0",
    "typescript": "^2.0",
    "@types/jasmine": "2.5.40",
    "@types/lodash-es": "^4.14.2",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.1",
    "better-npm-run": "0.0.14",
    "codelyzer": "2.0.0-beta.1",
    "css-loader": "^0.26.1",
    "finalhandler": "^0.5.1",
    "husky": "^0.12.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.1.0",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.8.1",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "^5.0.0",
    "protractor-jasmine2-screenshot-reporter": "^0.3.2",
    "raw-loader": "0.5.1",
    "serve-static": "^1.11.1",
    "source-map-loader": "^0.1.6",
    "standard-version": "^4.0.0",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "^2.0.0",
    "tslint": "4.0.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.3",
    "webpack": "2.2.0-rc.3",
    "xml2js": "^0.4.17"
  },
  "cordovaPlugins": [
    "cordova-plugin-console",
    "cordova-plugin-statusbar",
    "cordova-plugin-whitelist",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [],
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
  }
}
